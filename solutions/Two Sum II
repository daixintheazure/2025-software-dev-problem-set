public class Solution {
    public int[] TwoSum(int[] numbers, int target) {
        
        for(int i = 0; i < numbers.Count(); i++)
        {
            for(int j = 0; j < numbers.Count(); j++)
            {
                if(i != j)
                {
                    if(numbers[i] + numbers[j] == target)
                    {
                        return new int[] {i + 1, j + 1};
                    }
                }
            }
        }
        return null;
    }

}
